    	<typeAlias alias="Channellog_min" type="com.nebo_spider.entity.Channellog_min" />
    	<typeAlias alias="Columnlog_count" type="com.nebo_spider.entity.Columnlog_count" />
    	<typeAlias alias="Columnlog_day" type="com.nebo_spider.entity.Columnlog_day" />
    	<typeAlias alias="Columnlog_hour" type="com.nebo_spider.entity.Columnlog_hour" />
    	<typeAlias alias="Columnlog_min" type="com.nebo_spider.entity.Columnlog_min" />
    	
